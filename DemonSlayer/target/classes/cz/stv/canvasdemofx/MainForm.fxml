<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="780.0" prefWidth="1480.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cz.stv.canvasdemofx.MainForm">
   <children>
      <Canvas fx:id="canvas" height="600.0" layoutX="15.0" layoutY="15.0" onMouseDragged="#onMouseDragged" onMouseMoved="#onMouseMoved" onMousePressed="#onMousePressed" onMouseReleased="#onMouseReleased" width="1200.0" AnchorPane.bottomAnchor="121.0" AnchorPane.leftAnchor="15.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="14.0" />
      <Button fx:id="escape" layoutY="0.0" layoutX="1225.0" mnemonicParsing="false" onAction="#pauseTheGame" prefHeight="25.0" prefWidth="52.0" text="ESC" onKeyPressed="#onKeyPressed" onKeyReleased="#onKeyReleased"/>
      <Button fx:id="mashinegunbutton" layoutY="25.0" layoutX="1225.0" mnemonicParsing="false" onAction="#mashinegunMode" prefHeight="25.0" prefWidth="100.0" text="Mashinegun O"/>
      <Button fx:id="shotgunbutton" layoutY="50.0" layoutX="1225.0" mnemonicParsing="false" onAction="#shotgunMode" prefHeight="25.0" prefWidth="100.0" text="Shotgun P"/>
      <Button fx:id="fasterbulletsbutton" layoutY="75.0" layoutX="1225.0" mnemonicParsing="false" onAction="#fastBulletsMode" prefHeight="25.0" prefWidth="100.0" text="Faster shots I"/>
      <Button fx:id="fastwalkingbutton" layoutY="100.0" layoutX="1225.0" mnemonicParsing="false" onAction="#bounceMode" prefHeight="25.0" prefWidth="100.0" text="Bounce shots Z"/>
      <Button fx:id="bouncebutton" layoutY="125.0" layoutX="1225.0" mnemonicParsing="false" onAction="#fastWalkingMode" prefHeight="25.0" prefWidth="100.0" text="U Faster walking"/>
      <Button fx:id="autoshootingbutton" layoutY="150.0" layoutX="1225.0" mnemonicParsing="false" onAction="#autoShootingMode" prefHeight="25.0" prefWidth="100.0" text="Autoshoot C"/>
      <Button fx:id="piercingbulletsbutton" layoutY="175.0" layoutX="1225.0" mnemonicParsing="false" onAction="#piercingMode" prefHeight="25.0" prefWidth="100.0" text="Piercing"/>
      <Button fx:id="ultramashinegunbutton" layoutY="200.0" layoutX="1225.0" mnemonicParsing="false" onAction="#ultraMashinegunMode" prefHeight="25.0" prefWidth="100.0" text="X Mashinegun++"/>
      <Button fx:id="granateanablebutton" layoutY="225.0" layoutX="1225.0" mnemonicParsing="false" onAction="#granateMode" prefHeight="25.0" prefWidth="100.0" text="TNT"/>
   </children>
</AnchorPane>
